<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%highlight{%d{HH:mm:ss.SSS} [%t] %-5level %logger{1.} - %msg%n}"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="WARN">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="ch.uzh.ifi.seal.ase.cin.miner.Miner" level="trace" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="ch.uzh.ifi.seal.ase.cin.miner.ZipProcessor" level="trace" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>